config_version=5

[application]

config/name="GMir-4.1"
config/description="A game I wanna make."
config/tags=PackedStringArray("mygame", "textgame")
run/main_scene="res://main.tscn"
config/features=PackedStringArray("4.2", "Mobile")
boot_splash/show_image=false

[autoload]

dbc="*res://AutoLoad/db_command.gd"
G="*res://AutoLoad/globals.gd"

[debug]

settings/stdout/verbose_stdout=true
gdscript/warnings/get_node_default_without_onready=1

[display]

window/size/viewport_width=188
window/size/viewport_height=334
window/size/window_width_override=375
window/size/window_height_override=667
window/subwindows/embed_subwindows=false
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"
window/vsync/vsync_mode=0

[dotnet]

project/assembly_name="GMir-4.1"

[editor]

movie_writer/disable_vsync=true
naming/node_name_casing=2
export/convert_text_resources_to_binary=false

[editor_plugins]

enabled=PackedStringArray("res://addons/godot-sqlite/plugin.cfg")

[filesystem]

import/blender/enabled=false

