; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "State",
"language": "GDScript",
"path": "res://src/Commons/states.gd"
} ]
_global_script_class_icons={
"State": ""
}

[application]

config/name="MyGame"
run/main_scene="res://main.tscn"
config/icon="res://icon.png"

[autoload]

States="*res://src/Commons/states.gd"

[debug]

settings/stdout/verbose_stdout=true

[display]

window/size/height=768
window/stretch/mode="2d"
window/stretch/aspect="keep"

[layer_names]

2d_render/layer_1="world"
2d_physics/layer_1="world"
2d_render/layer_2="players"
2d_physics/layer_2="players"
2d_render/layer_3="objects"
2d_physics/layer_3="objects"
2d_render/layer_4="items"
2d_physics/layer_4="items"

[physics]

2d/thread_model=2

[rendering]

quality/driver/driver_name="GLES2"
threads/thread_model=2
environment/default_clear_color=Color( 0.160784, 0.160784, 0.160784, 0.780392 )
environment/default_environment="res://default_env.tres"
